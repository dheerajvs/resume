{
  "name": "jsonresume-theme-progressive",
  "version": "1.0.0",
  "description": "Progressive theme for JSON Resume using PWA, AMP and JSON-LD technologies",
  "main": "index.js",
  "scripts": {
    "start": "resume serve -r resume-dheeraj.json",
    "pug": "pug --pretty -O resume-dheeraj.json -w -o ./public templates/index.pug",
    "resume": "resume serve",
    "scss": "sass --watch styles:styles",
    "scss_prod": "sass --force --style compressed --update styles:styles",
    "serve": "live-server ./public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dheerajvs/resume.git"
  },
  "keywords": [
    "resume"
  ],
  "author": "Dheeraj V S",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dheerajvs/resume/issues"
  },
  "homepage": "https://github.com/dheerajvs/resume#readme",
  "devDependencies": {
    "live-server": "^1.1.0",
    "pug-cli": "^1.0.0-alpha6",
    "resume-cli": "^0.4.14"
  },
  "dependencies": {
    "pug": "^2.0.0-beta6",
    "qr-image": "^3.1.0"
  }
}
